{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "strongloop"],
  "rules": {
    "max-len": [2, 120, 8],
    "no-console": 0,
    "no-unused-vars": 0
  }
}